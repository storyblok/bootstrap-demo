{{ blok._editable }}
<div>
  {% if blok.image != blank %}
    <img alt="image" class="fdb-icon" src="{{ blok.image }}">
  {% endif %}
  <h3>{{ blok.headline }}</h3>
  <p>{{ blok.text }}</p>
  <p class="mt-3">
    <a class="btn" href="{{ blok.button_link | url }}">{{ blok.button_text }}</a>
  </p>
</div>
